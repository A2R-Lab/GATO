    <axis xyz="0 0 1"/>
    <limit lower="-2.094" upper="2.094" effort="300" velocity="10" />
    <safety_controller soft_lower_limit="-2.059" soft_upper_limit="2.059" k_position="100" k_velocity="2"/>
    <dynamics damping="0.0" friction="0.0"/>
